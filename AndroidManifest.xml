<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.example.mallk"
	android:versionCode="1"
	android:versionName="1.0" >

	<uses-sdk
		android:minSdkVersion="8"
		android:targetSdkVersion="19" />

	<uses-permission android:name="android.permission.INTERNET" />

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme" >
		<activity
			android:name="com.example.mallk.MainActivity"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="com.example.mallk.StoreDetailActivity"
			android:label="@string/title_activity_store_detail" >
		</activity>
		<activity
			android:name="com.example.mallk.StoreDetActivity"
			android:label="@string/title_activity_store_det" >
		</activity>
		<activity
			android:name="com.example.mallk.ImgActivity"
			android:label="@string/title_activity_img" >
		</activity>
	</application>

</manifest>
